{
  "name": "usmanzahid/laravel-custom-make",
  "description": "A Laravel package to generate any kind of file using custom stubs and configuration.",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "UsmanZahid\\LaravelCustomMake\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Usman Zahid",
      "email": "developerusman@yahoo.com"
    }
  ],
  "require": {
    "php": "^8.3|^8.2|^8.1|^8.0",
    "laravel/framework": "^12.0|^11.0|^10.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "UsmanZahid\\LaravelCustomMake\\CustomMakeProvider"
      ]
    }
  }
}
