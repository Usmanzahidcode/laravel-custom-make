{
  "name": "usmanzahid/laravel_custom_make",
  "description": "A package to create any kind of file defined by the user",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "UsmanZahid\\LaravelCustomMake\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Usman Zahid",
      "email": "developerusman@yahoo.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "8.3.*",
    "laravel/framework": "^11.0|^12.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "UsmanZahid\\LaravelCustomMake\\CustomMakeProvider"
      ]
    }
  }
}
